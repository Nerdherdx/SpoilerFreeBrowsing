{
  "manifest_version": 2,
  "name": "SpoilerFreeApplication",
  "version": "0.01",
  "icon": {
    "128": "spoiler.png"
  },
  "browser_action": {
    "default_icon": "spoiler.png",
    "default_popup": "Index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "spoilers.js"
      ]
    }
  ]
}